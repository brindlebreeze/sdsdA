m = Instance.new("Message")
m.Parent = game.Workspace
m.Text = "AHHHH A METEOR IS COMING RUN FOR YER LIVES"
wait(3)
m:Remove()
p = Instance.new("Part")
p.Size = Vector3.new(100,100,100)
p.BrickColor = BrickColor.new("Brown")
p.Parent = game.Workspace
p.Position = Vector3.new(0,251.6,0)
p.Material = "Slate"
p.Shape = "Ball"
wait(1)
e = Instance.new("Explosion")
e.Parent = game.Workspace.Part
